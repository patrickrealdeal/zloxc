var time = clock();

fun add(a, b) {
    return a + b;
}

fun mul(a, b) {
    return a * b;
}

fun elapsed(start) {
    return clock() - start;
}

fun main() {    
    print mul(2,3);
    print add(1,2);
    print elapsed(time);
    print elapsed(time);
    print elapsed(time);
    print elapsed(time);
    print elapsed(time);
    
    return 0;
}


print add(3,4);
